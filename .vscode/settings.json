{
  "azureFunctions.projectLanguage": "TypeScript",
  "azureFunctions.projectRuntime": "~4",
  "azureFunctions.projectSubpath": "src",
  "azureFunctions.preDeployTask": "Build with yarn",
  "azureFunctions.deploySubpath": "dist",
  "cSpell.words": [
    "AUTOINCREMENT",
    "CONNECTIONSTRING",
    "consts",
    "datetime",
    "devcontainer",
    "devcontainers",
    "errorlens",
    "funcignore",
    "FUNCTIONAPP",
    "Guilherme",
    "HOSTURL",
    "JDBC",
    "maxdepth",
    "MGBR",
    "Nektos",
    "Parens",
    "postbuild",
    "prestart",
    "REJECTUNAUTHORIZED",
    "serverlesscounter",
    "sonarcloud",
    "sonarlint",
    "sonarqube",
    "sonarsource",
    "SVRLSSCTR",
    "wtoc"
  ],
  "terminal.integrated.cwd": "${workspaceFolder}/src",
  "terminal.integrated.env.osx": {
    "ZSH_INIT_COMMAND_SVRLSSCTR": "source ../.vscode/dev-environment-init.sh"
  },
  "terminal.integrated.profiles.osx": {
    "zsh": {
      "path": "zsh",
      "args": ["-l"],
      "icon": "flame"
    }
  },
  "terminal.integrated.defaultProfile.osx": "zsh",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "jest.rootPath": "src",
  "jest.autoRun": "off"
}
