{
  "azureFunctions.projectLanguage": "TypeScript",
  "azureFunctions.projectRuntime": "~4",
  "azureFunctions.projectSubpath": "src",
  "azureFunctions.preDeployTask": "Build with yarn",
  "azureFunctions.deploySubpath": "dist",
  "cSpell.words": [
    "CONNECTIONSTRING",
    "consts",
    "devcontainers",
    "funcignore",
    "JDBC",
    "maxdepth",
    "Parens",
    "postbuild",
    "prestart",
    "REJECTUNAUTHORIZED",
    "serverlesscounter",
    "sonarcloud",
    "sonarqube",
    "sonarsource",
    "wtoc"
  ],
  "terminal.integrated.cwd": "${workspaceFolder}/src",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "jest.rootPath": "src",
  "jest.autoRun": "off"
}
